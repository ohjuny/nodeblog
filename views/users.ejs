<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>User List</h1>
    <ul>
      <%
        var list = '';
        for (i = 0; i < users.length; i++) {
          list += '<li>' + users[i].username + ': ' + users[i].email + '</li>';
        }
      %>
      <%- list %>
    </ul>
  </body>
</html>
